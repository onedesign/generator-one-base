{
  "name": "generator-one-base",
  "version": "2.0.0",
  "description": "A foundation for One Design Company projects on the web.",
  "homepage": "https://github.com/onedesign/generator-one-base",
  "author": {
    "name": "One Design Company",
    "email": "dev@onedesigncompany.com",
    "url": "https://onedesigncompany.com"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["", "yeoman-generator"],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "chalk": "^2.1.0",
    "deep-extend": "^0.5.0",
    "eslint-config-odc": "^1.1.0",
    "eslint-plugin-react": "^7.5.1",
    "install": "^0.10.4",
    "npm": "^5.6.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["odc"],
    "env": {
      "jest": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "no-var": "error",
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": true
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "git@github.com:onedesign/generator-one-base.git",
  "license": "MIT"
}
