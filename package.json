{
  "name": "generator-one-base",
  "version": "2.0.0",
  "description": "A foundation for One Design Company projects on the web.",
  "homepage": "https://github.com/onedesign/generator-one-base",
  "author": {
    "name": "One Design Company",
    "email": "dev@onedesigncompany.com",
    "url": "https://onedesigncompany.com"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "engines": {
    "node": ">=6.12.0",
    "npm": ">=5.7.0"
  },
  "keywords": ["", "yeoman-generator"],
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-odc": "^1.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^4.2.1",
    "prettier": "^1.12.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.2"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "deep-extend": "^0.5.1",
    "download": "^6.2.5",
    "guid": "^0.0.12",
    "install": "^0.10.4",
    "lodash": "^4.17.10",
    "npm": "^6.1.0",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/.jest-setup.js",
    "verbose": true,
    "collectCoverage": true
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "test:dev": "jest --watch --verbose -o"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["odc"],
    "env": {
      "jest": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "no-var": "error",
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": true
        }
      ]
    },
    "plugins": ["prettier", "ejs"]
  },
  "repository": "git@github.com:onedesign/generator-one-base.git",
  "license": "MIT"
}
