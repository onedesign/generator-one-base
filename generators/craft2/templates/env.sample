# IMAGER_URL="https://s3.amazonaws.com/<%= projectName %>/imager/"
# AWS_ACCESS_KEY=""
# AWS_SECRET_ACCESS_KEY=""
# AWS_BUCKET="<%= projectName %>"
# AWS_FOLDER="imager"<% } %>
